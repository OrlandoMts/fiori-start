<mvc:View controllerName="omontes.invoices.controller.MainView"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <content>
            <Text id="_IDGenText1" text="Hola Mundo"></Text>
            <Panel id="_IDGenPanel1" headerText="{i18n>filters}">
                <Label id="labelShipName" labelFor="shipName" text="Ship name" class="sapUiSmallMargin"></Label>
                <Input id="shipName" width="200px" class="sapUiSmallMarginEnd"></Input>

                <Label id="labelCountry" labelFor="country" text="Country" class="sapUiSmallMargin"></Label>
                <Input id="country" width="200px" class="sapUiSmallMarginEnd"></Input>
            </Panel>
        </content>
    </Page>
</mvc:View>
